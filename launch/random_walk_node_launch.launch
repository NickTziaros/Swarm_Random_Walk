<?xml version="1.0"?>
<launch>
	  <arg name="robot_name" default="robot"/>
	  <!--source devel/setup.bash && rosrun swarm PSO_SM_Node.py __ns:=robot_4 robot_4
 -->


	<group ns="$(arg robot_name)_1">
		<node pkg="random_walk" name="rw_node" type="rw_node.py" output="screen" args="$(arg robot_name)_1 ">
        </node> 

	</group>

	<group ns="$(arg robot_name)_2">
		<node pkg="random_walk" name="rw_node" type="rw_node.py" output="screen" args="$(arg robot_name)_2 ">
        </node> 

	</group>

	<group ns="$(arg robot_name)_3">
		<node pkg="random_walk" name="rw_node" type="rw_node.py" output="screen" args="$(arg robot_name)_3 ">
        </node> 

	</group>

	<group ns="$(arg robot_name)_4">
		<node pkg="random_walk" name="rw_node" type="rw_node.py" output="screen" args="$(arg robot_name)_4 ">
        </node> 

	</group>

	<group ns="$(arg robot_name)_5">
		<node pkg="random_walk" name="rw_node" type="rw_node.py" output="screen" args="$(arg robot_name)_5 ">
        </node> 

	</group>








</launch>